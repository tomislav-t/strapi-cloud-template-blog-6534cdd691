{
  "kind": "collectionType",
  "collectionName": "videos",
  "info": {
    "singularName": "video",
    "pluralName": "videos",
    "displayName": "Video"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Description": {
      "type": "text"
    },
    "VideoUrl": {
      "type": "string",
      "required": true
    },
    "video_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::video-category.video-category",
      "inversedBy": "videos"
    },
    "PublishFrom": {
      "type": "datetime"
    },
    "PublishTo": {
      "type": "datetime"
    },
    "NumberOfViews": {
      "type": "biginteger"
    },
    "Actions": {
      "type": "component",
      "component": "shared.action",
      "repeatable": true
    }
  }
}
